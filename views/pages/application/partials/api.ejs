                                   <span ng-bind="cat.cat_name"></span>
                                   <md-icon class="" md-svg-icon="navigation:ic_chevron_right_24px"></md-icon>
                               </md-button>
                               <md-menu-content class="md-dropdown-content nested" >
                                   <md-menu-item>
                                       <md-button ng-click="chooseCat(cat)">全部</md-button>
